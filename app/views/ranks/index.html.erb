<%- model_mst_date = MstDate -%>
<div class="page-header">
  <h1>Hits Stream</h1>
</div>

<ul id="result"></ul>

<%= form_tag('/ranks/result', :remote => true, :method => 'post'
) do %>
<%= select_tag(:this_date,options_from_collection_for_select(@mst_dates.order("id DESC"),:this_date,:this_date)
)%>
<%= select_tag(:name,options_from_collection_for_select(@mst_genres.order("id"),:name,:name)
)%>
<%= submit_tag("Select") %>
<% end %>

<div id='search_result'>
<%= render partial: 'list', locals: {contents: @view_rankings} %>
</div>