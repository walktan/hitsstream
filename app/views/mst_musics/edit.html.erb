<div class="page-header">
  <h1 id='h1'>Hits Stream</h1>
</div>

<div id='mv_list'>
	<p>VideoID</p>
	<%= form_for @mst_music do |f| %>
	<p>
		<%= f.text_field :youtube_url %>
	</p>
	<p>
		<%= f.hidden_field :target_date, :value => @target_date %>
		<%= f.hidden_field :genre, :value => @genre %>
		<%= f.hidden_field :now_ranking, :value => @now_ranking %>
		<%= f.submit 'UPDATE' %>
	</p>

	<% end %>
</div>

<div class="page-footer">
</div>

<%= javascript_tag do %>
$(document).ready(function(){
  $('#h1').click(function(){
    location.href = '/'
  });
});
<% end %>